{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "spgdatafactory12"
		},
		"spgazureprojectdb_ls_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'spgazureprojectdb_ls'"
		},
		"spgstorage_ls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'spgstorage_ls'"
		},
		"spgazureprojectdb_ls_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "spgazureproject.database.windows.net"
		},
		"spgazureprojectdb_ls_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "spgazureprojectdb"
		},
		"spgazureprojectdb_ls_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Adminspg"
		},
		"spgstorage_ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://spgstorage12.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "spgdatafactory12_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"spgazureprojectdb_ls_password": {
						"value": "[parameters('spgazureprojectdb_ls_password')]"
					},
					"spgstorage_ls_accountKey": {
						"value": "[parameters('spgstorage_ls_accountKey')]"
					},
					"spgazureprojectdb_ls_properties_typeProperties_server": {
						"value": "[parameters('spgazureprojectdb_ls_properties_typeProperties_server')]"
					},
					"spgazureprojectdb_ls_properties_typeProperties_database": {
						"value": "[parameters('spgazureprojectdb_ls_properties_typeProperties_database')]"
					},
					"spgazureprojectdb_ls_properties_typeProperties_userName": {
						"value": "[parameters('spgazureprojectdb_ls_properties_typeProperties_userName')]"
					},
					"spgstorage_ls_properties_typeProperties_url": {
						"value": "[parameters('spgstorage_ls_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}